                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564122002779;http://dx.doi.org/10.1016/j.dental.2022.09.016" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "38" ;
                                                                                                         :datePublished "2022" ;
                                                                                                         :keywords "Resin-based restoratives, Biostability, Restorative failure, Resin design, Methacrylates" ;
