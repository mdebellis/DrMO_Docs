                                                                                                         :cochraneGroupCode "Skin" ;
                                                                                                         :keywords "Dermatitis, Atopic [*diet therapy]; Egg Hypersensitivity [diet therapy]; Food, Formulated; Humans; Milk Hypersensitivity [diet therapy]; Randomized Controlled Trials as Topic" ;
                                                                                                         dcterms:creator "BathHextall, FJ; Delamere, FM; Williams, HC" ;
                                                                                                         rdfs:label "Dietary exclusions for established atopic eczema" .


###  http://www.semanticweb.org/ontologies/2022/titutuli/nivedita/drmo#8b698ddb-832b-4782-ba73-300ee93b6e0d
<http://www.semanticweb.org/ontologies/2022/titutuli/nivedita/drmo#8b698ddb-832b-4782-ba73-300ee93b6e0d> rdf:type owl:NamedIndividual ,
                                                                                                                  :Document ,
                                                                                                                  :JournalArticle ;
