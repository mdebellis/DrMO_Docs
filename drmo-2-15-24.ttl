                                                                                                         <http://purl.org/ontology/bibo/issn> "1465-1858" ;
                                                                                                         <http://purl.org/ontology/bibo/issue> "11" ;
                                                                                                         <http://purl.org/ontology/bibo/uri> "http://dx.doi.org/10.1002/14651858.CD009542.pub2" ;
                                                                                                         :cochraneGroupCode "Tobacco Addiction" ;
                                                                                                         :keywords "Administration, Oral; Adult; Animals; Child; Desensitization, Immunologic [adverse effects, *methods]; Humans; Milk Hypersensitivity [immunology, *therapy]; Milk [adverse effects, immunology]; Randomized Controlled Trials as Topic" ;
                                                                                                         dcterms:creator "Yeung, JP; Kloda, LA; McDevitt, J; BenShoshan, M; Alizadehfar, R" ;
