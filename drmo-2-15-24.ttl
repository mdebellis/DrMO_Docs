                                                                                                         <http://purl.org/ontology/bibo/issn> "0109-5641" ;
                                                                                                         <http://purl.org/ontology/bibo/issue> "3" ;
                                                                                                         <http://purl.org/ontology/bibo/pages> "477-485" ;
                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564118312004;http://dx.doi.org/10.1016/j.dental.2019.01.005" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "35" ;
                                                                                                         :keywords "Artificial saliva, Dental materials, Ethanol, , Liquid chromatography, Mass spectrometry, Monomers" ;
