                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564122000598;http://dx.doi.org/10.1016/j.dental.2022.02.011" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "38" ;
                                                                                                         :datePublished "2022" ;
                                                                                                         :keywords "Bioactivity, Sol-gel synthesis, Restorative dentistry, Dental material, Antibacterial, Cytotoxicity, pH, Ion release, Properties" ;
                                                                                                         dcterms:creator "Simila HO,Boccaccini AR" ;
