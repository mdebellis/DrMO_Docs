                                                                                                         <http://purl.org/ontology/bibo/issn> "0109-5641" ;
                                                                                                         <http://purl.org/ontology/bibo/issue> "8" ;
                                                                                                         <http://purl.org/ontology/bibo/pages> "997-1008" ;
                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564120301287;http://dx.doi.org/10.1016/j.dental.2020.04.019" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "36" ;
                                                                                                         :datePublished "2020" ;
                                                                                                         :keywords "Bulk fill, Resin composite, Irradiation regimes, Exposure duration, Reciprocity, Vickers hardness, Hardness depth profile" ;
                                                                                                         dcterms:creator "Sadeghyar A,Watts DC,Schedle A" ;
                                                                                                         rdfs:label "Limited reciprocity in curing efficiency of bulk-fill resin-composites" .


###  http://www.semanticweb.org/ontologies/2022/titutuli/nivedita/drmo#73ae63f3-810e-4113-a3e2-16b2e8adef6e
<http://www.semanticweb.org/ontologies/2022/titutuli/nivedita/drmo#73ae63f3-810e-4113-a3e2-16b2e8adef6e> rdf:type owl:NamedIndividual ,
