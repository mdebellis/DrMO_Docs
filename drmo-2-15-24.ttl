                                                                                                         <http://purl.org/ontology/bibo/issue> "8" ;
                                                                                                         <http://purl.org/ontology/bibo/uri> "http://dx.doi.org/10.1002/14651858.CD008935.pub2" ;
                                                                                                         :cochraneGroupCode "Emergency and Critical Care" ;
                                                                                                         :keywords "Adrenergic Agonists [*administration & dosage]; Anaphylaxis [*drug therapy]; Epinephrine [*administration & dosage]; Humans; Injections, Intramuscular [*instrumentation]; Self Administration [*instrumentation]" ;
                                                                                                         dcterms:creator "Sheikh, A; Simons, FER; Barbour, V; Worth, A" ;
