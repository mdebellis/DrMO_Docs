                                                                                                         <http://purl.org/ontology/bibo/issn> "0109-5641" ;
                                                                                                         <http://purl.org/ontology/bibo/pages> "e30-e31" ;
                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564123002440;http://dx.doi.org/10.1016/j.dental.2023.08.062" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "39" ;
