                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0950061823025850" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "400" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "3D Printing Geopolymer, Construction Binder, Mechanical, Activator" ;
