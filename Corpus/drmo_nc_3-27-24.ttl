                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564121002372" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "37" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Antimicrobial photodynamic therapy, CTX, ICTP, Indocyanine green, Microtensile bond strength, Toluidine Blue" ;
                                                                                                         :number "10" ;
                                                                                                         :yearPublished "2021" ;
                                                                                                         dcterms:creator "Kara, Ozlem; Seseogullari Dirihan, Roda; Sayin Ozel, Gulsum; Tezvergil Mutluay, Arzu; Usumez, Aslihan" ;
