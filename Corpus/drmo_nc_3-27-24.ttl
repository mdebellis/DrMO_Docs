                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564118306444" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "35" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "HEMA, Oral keratinocytes, Gingival fibroblasts, Gene expression, Oxidative stress, Inflammation, ECM" ;
                                                                                                         :number "3" ;
                                                                                                         :yearPublished "2019" ;
