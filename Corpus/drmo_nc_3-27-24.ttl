                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0029801823025295" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "288" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Cross-sea bridge foundation, Local scour, Scour protection mechanism, Passive scour countermeasure, Active scour countermeasure, Hybrid scour countermeasure, Scour protection resistance" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Chen, Jingjing; Qu, Yunfei; Sun, Zhilin" ;
