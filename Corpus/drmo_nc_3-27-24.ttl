                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564121001184" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "37" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Ceramics, Polymers, Resin, Zirconia, Lithium disilicate, Implant, Abutment, Crown" ;
                                                                                                         :number "8" ;
                                                                                                         :yearPublished "2021" ;
                                                                                                         dcterms:creator "Elsayed, Adham; Yazigi, Christine; Kern, Matthias; Chaar, Mohamed Sad" ;
