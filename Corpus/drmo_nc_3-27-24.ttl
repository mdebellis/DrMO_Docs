                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564123004086" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "39" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "3D printed resin, Titanium dioxide, Nanoparticles, Nanocomposite, Stereolithography (SLA), Additive manufacturing, Denture base, Martens hardness" ;
                                                                                                         :number "12" ;
                                                                                                         :yearPublished "2023" ;
