                                                                                                         <http://purl.org/ontology/bibo/volume> "38" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "CAD/CAM, Karl Fischer titration, Resin composite, Water sorption" ;
                                                                                                         :number "5" ;
                                                                                                         :yearPublished "2022" ;
