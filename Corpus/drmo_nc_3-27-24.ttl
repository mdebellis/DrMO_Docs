                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Oral restorations, Blood lead, Renal function, Nephrotoxicity" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Sun, Wansu; Dai, Bichong; Hong, Lilin; Zhang, Hengguo" ;
