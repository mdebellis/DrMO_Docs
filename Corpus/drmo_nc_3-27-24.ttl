                                                                                                         <http://purl.org/ontology/bibo/volume> "36" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Dentin bonding, MMP inhibitor, MDP, Interaction" ;
                                                                                                         :number "12" ;
