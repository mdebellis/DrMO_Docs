                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564119303434" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "35" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Resin-based composites, Light curing unit, Resin conversion, Cytotoxicity" ;
                                                                                                         :number "8" ;
                                                                                                         :yearPublished "2019" ;
