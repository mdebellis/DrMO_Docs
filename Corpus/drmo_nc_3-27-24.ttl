                                                                                                         <http://purl.org/ontology/bibo/volume> "78" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :number "4" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Heptonstall, N.; Scott-Warren, J.; Berman, R.; Filippiadis, D.; Bell, J." ;
