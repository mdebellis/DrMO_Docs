                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Additive manufacturing, 3D/4D printing, Smart materials, Bioprinting, Tissue regeneration" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Elhadad, Amir A.; Rosa-Sainz, Ana; Caete, Raquel; Peralta, Estela; Begines, Beln; Balbuena, Mario; Alcudia, Ana; Torres, Y." ;
