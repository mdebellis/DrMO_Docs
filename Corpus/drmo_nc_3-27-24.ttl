                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0272884223000779" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "49" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Zirconia, ZrO, Ceramic membranes, Sol-gel, Membrane fabrication, Wastewater" ;
                                                                                                         :number "6" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Bortot Coelho, Fabricio Eduardo; Magnacca, Giuliana; Boffa, Vittorio; Candelario, Victor M.; Luiten-Olieman, Mieke; Zhang, Wenjing" ;
