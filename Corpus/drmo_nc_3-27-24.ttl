                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S1751616122005100" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "138" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Resin-based composite, Pre-heating, Degree of conversion, Microhardness, Flexural strength, Post-gel shrinkage, Shrinkage stress" ;
                                                                                                         :yearPublished "2023" ;
