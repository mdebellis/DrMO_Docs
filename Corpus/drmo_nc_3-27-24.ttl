                                                                                                         <http://purl.org/ontology/bibo/pages> "2161-2171" ;
                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S1058274623004172" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "32" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Robotic, PSI, navigation, computer-assisted surgery, arthroplasty, shoulder, TSA" ;
