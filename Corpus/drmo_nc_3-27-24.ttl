                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564122001920" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "38" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "3D printed resin, ZrO2 nanoparticles, Cytotoxicity, Antimicrobial activity" ;
                                                                                                         :number "8" ;
