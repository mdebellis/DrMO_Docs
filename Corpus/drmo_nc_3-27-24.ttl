                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S2214790X23001533" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "16" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Sand mining, Urbanisation, Livelihood, Environmental degradation, Economic development" ;
                                                                                                         :yearPublished "2023" ;
