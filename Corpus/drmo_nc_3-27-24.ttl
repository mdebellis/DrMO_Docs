                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S1751616123005659" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "148" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Expanding monomer, Ring-opening polymerization, Polymerization shrinkage, Self-healing microcapsule, Antibacterial" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Zhang, Xiaoran; Zhang, Jiajia; Zhang, Ting; Yao, Shuo; Wang, Zonghua; Zhou, Chuanjian; Wu, Junling" ;
