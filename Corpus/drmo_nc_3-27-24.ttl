                                                                                                         <http://purl.org/ontology/bibo/volume> "38" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Dentin, Shear bond strength, Self-adhesive, Pretreatment" ;
                                                                                                         :number "11" ;
                                                                                                         :yearPublished "2022" ;
