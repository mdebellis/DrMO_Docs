                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S2949929123000359" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "1" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Soil improvement, Plants, Root reinforcement, Water-soluble polymer (WSP)" ;
                                                                                                         :number "3" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Li, Shanshan; Wang, Ziteng; Stutz, Hans Henning" ;
