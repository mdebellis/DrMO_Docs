                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S2772782322000869" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "5" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Sustainable energy systems, Circular economy, Energy management, Harvesting, Efficiency, Cleaner chemical engineering" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Seljak, Tine; Baleta, Jakov; Mikuli, Hrvoje" ;
