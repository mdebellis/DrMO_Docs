                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0013935123008204" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "231" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Landfill leachate concentrate (LLC), Geopolymer solidification (GS), Sustainability, LLC resource Recycling, Engineering application" ;
                                                                                                         :yearPublished "2023" ;
