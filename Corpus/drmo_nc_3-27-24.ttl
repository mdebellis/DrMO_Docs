                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564120301408" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "36" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "resin composites, fluorapatite, strength, fracture toughness, fluoride release" ;
                                                                                                         :number "8" ;
