                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S1773224723002538" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "84" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Implant, Surface modification, Nanostructures, Polymers, Antibiotic, Bioactive materials" ;
