                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564121001238" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "37" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Nano-CaF, Spray-drying, Co-precipitation, Dental composite, Fluoride release, Mechanical properties" ;
                                                                                                         :number "6" ;
                                                                                                         :yearPublished "2021" ;
                                                                                                         dcterms:creator "Dai, Quan; Weir, Michael D.; Ruan, Jianping; Liu, Jin; Gao, Jianghong; Lynch, Christopher D.; Oates, Thomas W.; Li, Yuncong; Chang, Xiaofeng; Xu, Hockin H. K." ;
