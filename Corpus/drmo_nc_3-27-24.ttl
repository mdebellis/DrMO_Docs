                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S1005030222007988" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "143" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Liquid metals, Synthesis, Composites, Antitumor therapy, Bio-application" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Guo, Zhenhu; Gao, Xiaohan; Lu, Jingsong; Xu, Wanling; Ye, Jielin; Li, Ying; Xie, Wensheng; Zhao, Lingyun" ;
