                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S010956412200344X" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "39" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Adhesion, Bonding, Silane, Fiber-reinforced composite, Interface, Fiber, Resin luting material, Semi-interpenetrating polymer network (semi-IPN)" ;
                                                                                                         :number "2" ;
                                                                                                         :yearPublished "2023" ;
