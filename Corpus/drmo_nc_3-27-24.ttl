                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S2213597923000861" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "32" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Photoacoustics, Ultrasound, Spectroscopy, Diagnostics, Clinical, Endogenous, Image-guided, Interventions, Theranostics" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "John, Samuel; Hester, Scott; Basij, Maryam; Paul, Avijit; Xavierselvan, Marvin; Mehrmohammadi, Mohammad; Mallidi, Srivalleesha" ;
