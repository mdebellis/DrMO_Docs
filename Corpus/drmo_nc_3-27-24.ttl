                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564121001111" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "37" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "3Y-TZP, Computer-aided design/computer-aided manufacturing, Raman spectroscopy, transformation, Aging, Biomaterials" ;
                                                                                                         :number "7" ;
                                                                                                         :yearPublished "2021" ;
