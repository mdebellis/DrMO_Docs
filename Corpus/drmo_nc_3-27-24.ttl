                                      <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0926580522005635" ;
                                      <http://purl.org/ontology/bibo/volume> "146" ;
                                      :bibliographyType "7" ;
                                      :keywords "Additive manufacturing, 3D concrete printing, 3D printers, Building materials, Cementitious materials, Alkali-activated materials, Geopolymers" ;
                                      :yearPublished "2023" ;
                                      dcterms:creator "Robayo-Salazar, Rafael; Meja de Gutirrez, Ruby; Villaquirn-Caicedo, Mnica A.; Delvasto Arjona, Silvio" ;
