                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Adsorption mechanism, Clays, Dyes, Heavy metals, Regeneration, Specific surface area" ;
                                                                                                         :number "11" ;
                                                                                                         :yearPublished "2023" ;
