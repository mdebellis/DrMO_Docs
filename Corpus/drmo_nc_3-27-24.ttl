                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S1773224723006573" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "87" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Zoledronic acid, NPs, Cancer therapy, Nanomedicine" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Fathi-karkan, Sonia; Zeeshan, Mahira; Qindeel, Maimoona; Eshaghi Malekshah, Rahime; Rahdar, Abbas; Ferreira, Luiz Fernando Romanholo" ;
