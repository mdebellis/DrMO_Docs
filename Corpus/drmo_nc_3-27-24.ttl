                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564120302797" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "36" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Cross-Linkers, Dentin bonding systems, Matrix metalloproteinases, N,N'-dicyclohexylcarbodiimide, Universal adhesives" ;
                                                                                                         :number "12" ;
                                                                                                         :yearPublished "2020" ;
