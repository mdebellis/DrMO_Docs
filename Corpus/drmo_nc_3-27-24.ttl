                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Microbial granulation technologies, Full-scale applications, Microbial interactions, EPS, Quorum sensing, Recovery of bioproducts" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Sethi, Shradhanjali; Gupta, Rohan; Bharshankh, Ankita; Sahu, Rojalin; Biswas, Rima" ;
