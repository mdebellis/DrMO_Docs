                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S2405844023035168" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "9" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Bone regeneration, Inorganic nano-structure, Osteo-immunomodulation, Osteo-inductive biomolecules, Macrophage polarization" ;
                                                                                                         :number "6" ;
                                                                                                         :yearPublished "2023" ;
                                                                                                         dcterms:creator "Mofarrah, Mohsen; Jafari-Gharabaghlou, Davoud; Sefidan-Jadid, Mahdi Farhoudi; Zarghami, Nosratollah" ;
