                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564120300816" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "36" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Dental resin composites, Zn-doping, Mesoporous SiO, Mechanical properties, Antibacterial activity" ;
                                                                                                         :number "6" ;
                                                                                                         :yearPublished "2020" ;
