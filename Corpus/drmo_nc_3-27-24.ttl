                                                                                                         <http://purl.org/ontology/bibo/uri> "https://www.sciencedirect.com/science/article/pii/S0109564121002633" ;
                                                                                                         <http://purl.org/ontology/bibo/volume> "37" ;
                                                                                                         :bibliographyType "7" ;
                                                                                                         :keywords "Dental materials, Fiber-reinforced composite, Resin-matrix ceramics, Dental prosthesis, Dental implants" ;
                                                                                                         :number "12" ;
